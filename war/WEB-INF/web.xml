<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
    <servlet>
    <servlet-name>FindPathServiceImpl</servlet-name>
    <servlet-class>de.htwmaps.server.FindPathServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>FindPathServiceImpl</servlet-name>
    <url-pattern>/htwmaps/findPath</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>CityStreetSuggestServiceImpl</servlet-name>
    <servlet-class>de.htwmaps.server.CityStreetSuggestServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>CityStreetSuggestServiceImpl</servlet-name>
    <url-pattern>/htwmaps/cityStreetSuggest</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>HtwMaps.html</welcome-file>
  </welcome-file-list>

</web-app>
